<p id="notice"><%= notice %></p>

<h1>Ruby Data</h1>

<table>
  <thead>
    <tr>
      <th>Valinput</th>
      <th>Rubytype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ruby_data.each do |ruby_datum| %>
      <tr>
        <td><%= ruby_datum.valInput %></td>
        <td><%= ruby_datum.rubyType %></td>
        <td><%= link_to 'Show', ruby_datum %></td>
        <td><%= link_to 'Edit', edit_ruby_datum_path(ruby_datum) %></td>
        <td><%= link_to 'Destroy', ruby_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ruby Datum', new_ruby_datum_path %>
